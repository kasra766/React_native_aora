    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-navigation": "^3.0.8",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.3.3"
